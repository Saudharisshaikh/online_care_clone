<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_gray"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/topBar"
        android:layout_alignParentTop="true"
        android:background="@color/theme_red"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/ivClose"
            android:layout_gravity="right"
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_close_white_24dp"/>

        <TextView
            android:id="@+id/dialogTittle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:textColor="#FFFFFF"
            android:text="Patient Details"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="normal" />
    </FrameLayout>

    <ScrollView
        android:id="@+id/svPatientDetails"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:focusableInTouchMode="true"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/lin"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_margin="10dp"
                android:orientation="horizontal" >

                <com.mikhaellopez.circularimageview.CircularImageView
                    android:id="@+id/imgSelPtImage"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_gravity="top"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/icon_call_screen"
                    app:border="true"
                    app:border_color="#FFFFFF"
                    app:border_width="1sp"
                    app:shadow="true" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvSelPtName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="20dp"
                        android:textStyle="normal" />
                    <LinearLayout
                        android:id="@+id/patientInfo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tvSelPtPhone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginTop="3dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            />
                        <TextView
                            android:id="@+id/tvSelPtEmail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginTop="3dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            />
                        <TextView
                            android:id="@+id/tvSelPtDOB"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginTop="3dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            />
                        <TextView
                            android:id="@+id/tvSelPtAddress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginTop="3dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/cust_border_white_outline"
                android:padding="10sp"
                android:orientation="vertical">
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="5dp"
                        android:text="Primary Diagnosis"
                        android:textStyle="bold"
                        android:textColor="@color/theme_red"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <ImageView
                        android:visibility="gone"
                        android:id="@+id/ivExpendExamLay"
                        android:padding="6dp"
                        android:layout_gravity="right|center_vertical"
                        android:src="@drawable/ic_add_box_black_24dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </FrameLayout>
                <!--<com.github.aakira.expandablelayout.ExpandableRelativeLayout
                    android:id="@+id/layExpandExam"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:ael_duration="1000"
                    app:ael_interpolator="fastOutSlowIn">
                </com.github.aakira.expandablelayout.ExpandableRelativeLayout>-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="Chief Complaints:"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvSymptomDetails"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="these are symptom details"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Symptoms:"
                            android:layout_margin="5dp"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvPtSymptom"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="these are symptoms"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="Symptom Explanation:"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvPain"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="these are pain"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="Condition:"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvPtCondition"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="these are conditions"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="Information related to your condition:"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvPtDescription"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="these are Description"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="Area of pain related to symptoms:"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvPainBodyPart"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="None"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="Level of Pain:"
                            android:textColor="@color/theme_red"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                        <TextView
                            android:id="@+id/tvPainSeverity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:text="these are pain severity"
                            android:textAppearance="?android:attr/textAppearanceMedium" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:padding="10dp"
                        android:background="@drawable/cust_border"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical" >
                        <FrameLayout
                            android:layout_marginTop="6dp"
                            android:layout_marginBottom="6dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Vitals"
                                android:textColor="@color/theme_red"
                                android:textAppearance="?android:attr/textAppearanceMedium" />
                            <TextView
                                android:id="@+id/tvVitalsDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:text=""
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="Temperature"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTTemperature"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border_white_outline"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="Temperature"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:focusable="false"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginRight="3dp"
                                android:layout_marginLeft="3dp"
                                android:gravity="center_horizontal"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="Height"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTHeight"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border_white_outline"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="Height"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:focusable="false"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="Weight"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTWeight"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border_white_outline"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="Weight"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:focusable="false"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="BP"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTBP"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border"
                                    android:focusable="false"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="BP"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_marginLeft="2dp"
                                android:layout_marginRight="2dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="HR"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTHR"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border"
                                    android:focusable="false"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="HR"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="Blood Sugar"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTBloodSugar"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border"
                                    android:focusable="false"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="Blood Sugar"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="BMI"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTBMI"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border"
                                    android:focusable="false"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="BMI"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginLeft="2dp"
                                android:layout_marginRight="2dp"
                                android:gravity="center_horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="11sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="Respirations"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTRespirations"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border"
                                    android:focusable="false"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="Resp"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="4dp"
                                    android:text="O2 Saturations"
                                    android:textSize="11sp"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />

                                <com.app.omrandr.util.ActionEditText
                                    android:id="@+id/etOTO2Saturations"
                                    android:imeOptions="actionDone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cust_border"
                                    android:focusable="false"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:hint="O2 Saturations"
                                    android:inputType="text"
                                    android:padding="10dp"
                                    android:textCursorDrawable="@null" />
                            </LinearLayout>
                        </LinearLayout>


                    </LinearLayout>

                    <TextView
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Images"
                        android:textColor="@color/theme_red"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                    <com.app.omrandr.util.ExpandableHeightGridView
                        android:id="@+id/gvReportImages"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:scrollbars="none"
                        android:numColumns="3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#e5e5e5"
                        android:horizontalSpacing="5dp"
                        android:stretchMode="columnWidth"
                        android:verticalSpacing="5dp"
                        >
                        <!--android:padding="5dp"-->
                    </com.app.omrandr.util.ExpandableHeightGridView>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/cust_border_white_outline"
                android:padding="10sp"
                android:orientation="vertical">
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tvHistoryBoxTittle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="5dp"
                        android:text="Patient Medical History"
                        android:textStyle="bold"
                        android:textColor="@color/theme_red"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <ImageView
                        android:visibility="gone"
                        android:id="@+id/ivExpendHistoryLay"
                        android:padding="6dp"
                        android:layout_gravity="right|center_vertical"
                        android:src="@drawable/ic_add_box_black_24dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </FrameLayout>
                <!--<com.github.aakira.expandablelayout.ExpandableRelativeLayout
                    android:id="@+id/layExpandHistory"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:ael_duration="1000"
                    app:ael_interpolator="fastOutSlowIn">

                </com.github.aakira.expandablelayout.ExpandableRelativeLayout>-->
                <androidx.core.widget.NestedScrollView
                    android:id="@+id/nsvHistory"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
